
    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:shrinkColumns="*"  android:stretchColumns="*" android:background="#ffffff">

    <LinearLayout android:layout_width="match_parent"
        android:layout_height="75dp">
        <ImageView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:id="@+id/imageView"
            android:layout_gravity="left"
            android:background="@drawable/iiitm"/>
        <ImageView
            android:layout_width="100dp"
            android:layout_height="match_parent"
            android:id="@+id/imageView1"
            android:layout_gravity="right"
            android:layout_marginLeft="120dp"
            android:background="@drawable/iii"/>
    </LinearLayout>

<LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
android:layout_marginLeft="100dp"
        android:id="@+id/textView"
        android:layout_gravity="center"
        android:background="@drawable/iciis"/>
    </LinearLayout>
    <TableRow
        android:id="@+id/tableRow1"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:layout_gravity="center"
        android:layout_marginTop="50dp">

        <TextView
            android:id="@+id/TextView01" android:text="PaperID"
            android:layout_weight="1" android:background="#dcdcdc"
            android:textColor="#000000"
          android:gravity="center"/>

        <TextView
            android:id="@+id/TextView02" 
            android:layout_weight="1" android:background="#d3d3d3"
            android:textColor="#000000"
             android:gravity="center"/>

       

    </TableRow>

    <TableRow
        android:id="@+id/tableRow2"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">

        <TextView
            android:id="@+id/TextView07" android:text="Date"
            android:layout_weight="1" android:background="#dcdcdc"
            android:textColor="#000000"
            android:gravity="center"/>

        <TextView
            android:id="@+id/TextView08" 
            android:layout_weight="1" android:background="#d3d3d3"
            android:textColor="#000000"
            android:gravity="center"/>

       

    </TableRow>
    <TableRow
        android:id="@+id/tableRow3"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">

        <TextView
            android:id="@+id/TextView04" android:text="Time"
            android:layout_weight="1" android:background="#dcdcdc"
            android:textColor="#000000"
            android:gravity="center"/>

        <TextView
            android:id="@+id/TextView05" 
            android:layout_weight="1" android:background="#d3d3d3"
            android:textColor="#000000"
            android:gravity="center"/>

      
    </TableRow>
    <TableRow
        android:id="@+id/tableRow5"
        android:layout_height="wrap_content"
        android:layout_width="match_parent">

        <TextView
            android:id="@+id/TextView41" android:text="RoomNo."
            android:layout_weight="1" android:background="#dcdcdc"
            android:textColor="#000000"
             android:gravity="center"/>

        <TextView
            android:id="@+id/TextView52"
            android:layout_weight="1" android:background="#d3d3d3"
            android:textColor="#000000"
           android:gravity="center"/>

        
    </TableRow>
</TableLayout>
